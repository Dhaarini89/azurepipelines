{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-abc8"
		},
		"linked_datalakegen_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'linked_datalakegen'"
		},
		"linked_sqldb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'linked_sqldb'"
		},
		"linked_synapsesqlpool_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'linked_synapsesqlpool'"
		},
		"linked_datalakegen_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsnycpayrolldhaarini.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-abc8_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"linked_datalakegen_accountKey": {
						"value": "[parameters('linked_datalakegen_accountKey')]"
					},
					"linked_sqldb_connectionString": {
						"value": "[parameters('linked_sqldb_connectionString')]"
					},
					"linked_synapsesqlpool_connectionString": {
						"value": "[parameters('linked_synapsesqlpool_connectionString')]"
					},
					"linked_datalakegen_properties_typeProperties_url": {
						"value": "[parameters('linked_datalakegen_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-abc8_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-abc8_ArmTemplate_0')]"
			]
		}
	]
}